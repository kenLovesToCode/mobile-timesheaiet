{
  "name": "pricetag",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "npm@11.8.0",
  "scripts": {
    "start": "expo start",
    "start:fresh": "npx expo start --clear",
    "start:prod": "npx expo start --no-dev --minify",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check:expo-config": "expo config --type public",
    "build": "expo export --platform web --platform ios --platform android",
    "build:production-router": "PRICETAG_ROUTER_ROOT=app-production expo export --platform web --platform ios --platform android",
    "test:navigation-smoke": "jest __tests__/story-1-4-navigation-smoke.test.js --runInBand --watchman=false",
    "typecheck": "tsc --noEmit",
    "lint": "expo lint"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "2.0.0-rc.17",
    "@tamagui/config": "2.0.0-rc.17",
    "@tamagui/font-inter": "2.0.0-rc.17",
    "@tamagui/native": "2.0.0-rc.17",
    "drizzle-orm": "^0.45.1",
    "expo": "55.0.0-preview.12",
    "expo-camera": "~55.0.7",
    "expo-haptics": "~55.0.7",
    "expo-linking": "~55.0.6",
    "expo-router": "55.0.0-preview.9",
    "expo-sqlite": "~55.0.8",
    "expo-status-bar": "~55.0.3",
    "react": "19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "0.83.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-worklets": "^0.7.4",
    "tamagui": "2.0.0-rc.17",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tamagui/babel-plugin": "2.0.0-rc.17",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.2.10",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "~55.0.6",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.39.3",
    "eslint-config-expo": "~55.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~55.0.9",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
